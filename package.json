{
  "name": "js-randomness-predictor-cpp",
  "version": "1.0.18",
  "description": "Node.js native addon (C++) for high-performance Math.random() prediction",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    "import": "./index.mjs",
    "require": "./index.cjs",
    "types": "./index.d.ts"
  },
  "binary": {
    "napi_versions": [
      8
    ],
    "module_name": "js-randomness-predictor-cpp",
    "host": "https://github.com/matthewoestreich/js-randomness-predictor-cpp/releases/download/"
  },
  "files": [
    "prebuilds/",
    "binding.gyp",
    "index.cjs",
    "index.mjs",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "preinstall": "prebuild-install || echo \"Falling back to build from source\"",
    "prebuild": "prebuild --backend node-gyp --verbose && prebuild --verbose --backend node-gyp",
    "configure": "node-gyp configure",
    "configure:verbose": "node-gyp configure --verbose",
    "build": "node-gyp build",
    "build:bear": "bear --output build/compile_commands.json -- node-gyp configure build",
    "clean": "node-gyp clean",
    "publish:main:major": "npm version major && npm publish && git push origin main --tags",
    "publish:main:minor": "npm version minor && npm publish && git push origin main --tags",
    "publish:main:patch": "npm version patch && npm publish && git push origin main --tags",
    "format": "node scripts/clang-format.mjs",
    "test": "node scripts/build-and-run-tests.mjs"
  },
  "keywords": [
    "predict javascript randomness",
    "predict math.random",
    "math.random",
    "math.random predictor",
    "predict",
    "predictor",
    "z3"
  ],
  "author": "Matt Oestreich",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/matthewoestreich/js-randomness-predictor-cpp.git"
  },
  "dependencies": {
    "node-addon-api": "^8.4.0",
    "node-gyp": "^11.2.0",
    "node-gyp-build": "^4.8.4",
    "prebuild-install": "^7.1.3"
  },
  "devDependencies": {
    "prebuild": "^13.0.1"
  }
}
